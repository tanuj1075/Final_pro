{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/$",
      "dest": "/api/index.php?route=login.php"
    },
    {
      "src": "^/admin/?$",
      "dest": "/api/index.php?route=index.php"
    },
    {
      "src": "^/(index|login|signup|ash|user_panel|oauth_start|oauth_callback)\\.php$",
      "dest": "/api/index.php?route=$1.php"
    },
    {
      "src": "^/(login|signup|admin|ash|user_panel|oauth_start|oauth_callback)/?$",
      "dest": "/api/index.php?route=$1"
    },
    {
      "src": "^/api/?$",
      "dest": "/api/index.php"
    },
    {
      "src": "^/(.*)$",
      "dest": "/$1"
    }
  ]
}
